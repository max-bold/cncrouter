    "version": "0.2.0",
    "configurations": [
        {
            "name": "RUN",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "root.trials.tryv3d",
            "justMyCode": false,
        },
        {
            "name": "bmvectortest",
            "type": "debugpy",
            "request": "launch",
            "module": "bmvector.tests.bmvectortest"
        },
        {
            "name": "arc.py",
            "type": "debugpy",
            "request": "launch",
            "module": "bmvector.arc"
        },
        {
            "name": "debug.py",
            "type": "debugpy",
            "request": "launch",
            "module": "bmvector.debug"
        },
        {
            "name": "MUPY",
            "type": "debugpy",
            "request": "launch",
            "module": "mypy",
            "args": [
                "${file}"
            ],
            "console": "integratedTerminal"
        }
    ]
}